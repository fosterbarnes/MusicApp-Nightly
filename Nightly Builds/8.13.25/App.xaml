<Application x:Class="MusicApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MusicApp"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <!-- SVG Icon Resources -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/Resources/svg/repeat-variant.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/svg/repeat-variant-1.xaml" />
                <ResourceDictionary Source="pack://application:,,,/Resources/svg/queue.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
